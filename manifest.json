{
  "manifest_version": 3,
  "name": "Mano Dienynas Enhance",
  "version": "1.0",
  "description": "Adds a print button to the homework page on manodienynas.lt.",
  "permissions": ["scripting"],
  "host_permissions": [
    "https://www.manodienynas.lt/*"
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.manodienynas.lt/*"
      ],
      "js": ["content.js"]
    }
  ]
}
