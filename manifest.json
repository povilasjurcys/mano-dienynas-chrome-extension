{
  "manifest_version": 3,
  "name": "Mano Dienynas Enhance",
  "version": "2.0",
  "description": "Collects homework data from all children and creates a combined printable view.",
  "permissions": ["scripting", "storage"],
  "host_permissions": [
    "https://www.manodienynas.lt/*"
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.manodienynas.lt/*"
      ],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
